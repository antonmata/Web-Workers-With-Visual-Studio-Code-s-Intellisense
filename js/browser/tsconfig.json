{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "removeComments": false,
    "experimentalDecorators": true,
    "module": "commonjs",
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
